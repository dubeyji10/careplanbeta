  <script>
    if ("geolocation" in navigator) {
      navigator.geolocation.getCurrentPosition(function (position) {
        let latitude = position.coords.latitude;
        let longitude = position.coords.longitude;
        let localTime = new Date().toLocaleString();
        // Use latitude and longitude
        console.log('we need to store your device info such as coordinates and user-agent');
        console.log(' info stored : agent - ', navigator.userAgent, ' lat - ', latitude, ' long - ', longitude);
        // once user accepts allow location --it should run and update deviceInfo input

        document.getElementById('userInfo').value = `{"latitude":${latitude} , "longitude":${longitude} , "userAgent":${navigator.userAgent},"userDeviceLocalTime":${localTime}}`;
      });
    } else {
      console.log('we could not store device info such as coordinates and user-agent');
      // Handle case where geolocation is not available
      // document.getElementById('deviceInfo').value = `{"latitude":"NONE" , "longitude":"NONE" , "userAgent":"NONE"}`;
      document.getElementById('userInfo').value = `{"userAgent":${navigator.userAgent},"userDeviceLocalTime":${localTime}}`;

    }
  </script>

